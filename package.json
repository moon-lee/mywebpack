{
  "name": "mywebpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "all": "npm-run-all --sequential build deploy",
    "css": "npm-run-all --parallel css-clean css-lint css-compile --sequential css-prefix css-minify css-deploy",
    "css-clean": "shx rm -rf dist/css/custom.* && shx echo css clean done",
    "css-deploy": "shx rm -rf ../mywebci/assets/css/custom.* && shx cp -R dist/css/custom.* ../mywebci/assets/dist/css && shx echo css deploy done",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 src/scss/mycss.scss dist/css/custom.css",
    "css-lint": "stylelint --config .stylelintrc --syntax scss \"src/scss/**/*.scss\" --fix",
    "css-prefix": "postcss --config postcss.config.js --replace \"dist/css/*.css\" \"!dist/css/*.min.css\"",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output dist/css/custom.css dist/css/custom.css",
    "build": "webpack --progress --profile --display-error-details",
    "clean": "shx rm -rf ./dist && shx echo clean done",
    "deploy": "shx rm -rf ../mywebci/assets/dist && shx cp -R ./dist ../mywebci/assets/dist && shx echo deploy done"
  },
  "keywords": [],
  "author": "moon lee <lee.moonkyu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "clean-css-cli": "^4.2.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "shx": "^0.3.2",
    "stylelint": "^9.5.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-regular-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "bootstrap": "^4.1.3",
    "fullcalendar": "^3.9.0",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "popper.js": "^1.14.4"
  }
}
